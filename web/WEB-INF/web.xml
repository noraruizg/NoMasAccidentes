<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agregarContrato</servlet-name>
        <servlet-class>servlet.agregarContrato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listarContrato</servlet-name>
        <servlet-class>servlet.listarContrato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servlet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cliente</servlet-name>
        <servlet-class>servlet.cliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profesional</servlet-name>
        <servlet-class>servlet.profesional</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agregarContrato</servlet-name>
        <url-pattern>/agregarContrato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listarContrato</servlet-name>
        <url-pattern>/listarContrato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cliente</servlet-name>
        <url-pattern>/cliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profesional</servlet-name>
        <url-pattern>/profesional</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>listarUsuario</welcome-file>
    </welcome-file-list>
</web-app>
